  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drexel-ue/pinterest_clone/issues"
  },
  "homepage": "https://github.com/drexel-ue/pinterest_clone#readme",
  "dependencies": {
    "@types/faker": "^4.1.5",
    "@types/puppeteer": "^1.19.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.6.9",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "puppeteer": "^1.19.0",
    "react-masonry-component": "^6.2.1",
    "validator": "^11.1.0",
    "webpack-dev-server": "3.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
